            @Html.DisplayNameFor(model => model.divisions.NamaDivisi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.employees.Nama)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.employees.BirthDate)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.employees.Address)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.employees.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.employees.Phone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.divisions.NamaDivisi)
    </td>
    <td>
        @Html.ActionLink("Update", "Update", new { id = item.employees.employeeId}) |
        @Html.ActionLink("Details", "Details", new { id = item.employees.employeeId }) |
