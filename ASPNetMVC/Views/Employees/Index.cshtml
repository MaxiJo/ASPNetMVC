@model IEnumerable<ASPNetMVC.Models.ViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.employees.Nama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.employees.BirthDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.employees.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.employees.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.employees.Phone)
        </th>
